#!/usr/bin/python3

from sys import argv

def sumArgs():
    sum = 0
    length = len(argv)

    if length == 1:
        print("{}".format(0))
        return

    elif length == 2:
        print("{}".format(argv[1]))
        return

    else:
        for args in range(1, length):
            sum = sum + int(argv[args])
        print("{}".format(sum))
        return

if __name__ == "__main__":
    sumArgs()
